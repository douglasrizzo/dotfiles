{
    "autoDocstring.docstringFormat": "sphinx",
    "C_Cpp.clang_format_fallbackStyle": "Chromium",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorSurroundingLines": 3,
    "editor.detectIndentation": true,
    "editor.fontFamily": "'B612 Mono', 'monospace', 'Font Awesome 5 Free', 'Font Awesome 5 Free Solid', 'Font Awesome 5 Brands'",
    "editor.fontSize": 15,
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.suggestSelection": "first",
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "git.autofetch": true,
    "latex-utilities.tikzpreview.enabled": false,
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.view.pdf.viewer": "external",
    "python.analysis.cacheFolderPath": "~/.vscode-python-cache",
    "python.autoComplete.addBrackets": true,
    "python.dataScience.askForKernelRestart": false,
    "python.formatting.provider": "yapf",
    "python.jediEnabled": false,
    "python.linting.mypyEnabled": true,
    "python.pythonPath": "/home/dodo/.anaconda3/bin/python",
    "python.venvPath": "~/.virtualenvs",
    "spellright.addToSystemDictionary": false,
    "terminal.external.linuxExec": "alacritty",
    "workbench.activityBar.visible": false,
    "workbench.iconTheme": "ayu",
    "workbench.startupEditor": "newUntitledFile",
    "latex-workshop.latex.clean.fileTypes": [
        "*.acn",
        "*.acr",
        "*.alg",
        "*.aux",
        "*.bak*",
        "*.bbl",
        "*.bcf",
        "*.blg",
        "*.dvi",
        "*.fdb_latexmk",
        "*.fls",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.glsdefs",
        "*.glstex",
        "*.hd",
        "*.idx",
        "*.ilg",
        "*.ind",
        "*.ins",
        "*.ist",
        "*.loa",
        "*.loe",
        "*.lof",
        "*.log",
        "*.lot",
        "*.mw",
        "*.nav",
        "*.out",
        "*.ps",
        "*.run.xml",
        "*.sbl",
        "*.slg",
        "*.slo",
        "*.sls",
        "*.snm",
        "*.sym",
        "*.synctex.gz",
        "*.toc",
        "*.vrb",
        "*.xdy"
    ],
    "spellright.language": [
        "en_US",
        "pt_BR"
    ],
    "spellright.documentTypes": [
        "markdown",
        "plaintext",
        "latex"
    ],
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-pdf",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "makeglossaries",
            "command": "makeglossaries",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "bib2gls",
            "command": "bib2gls",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "texindy",
            "command": "texindy",
            "args": [
                "%DOCFILE%.idx"
            ],
            "env": {}
        }
    ],
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[latex]": {
        "editor.formatOnSave": false
    },
    "[python]": {
        "editor.formatOnSave": false
    },
    "python.languageServer": "Pylance",
    "editor.tabSize": 2,
    "workbench.colorTheme": "Wal",
    "window.zoomLevel": 0,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "python.analysis.completeFunctionParens": true,
    "latex-workshop.latexindent.path": "/home/dodo/.texlive/2020/bin/x86_64-linux/latexindent",
    "latex.server.autoDownload": true,
    "ltex.additionalRules.motherTongue": "pt-BR"
}
