{
    "activitusbar.alignment": "Right",
    "editor.detectIndentation": true,
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "latex-workshop.latex.autoBuild.onSave.enabled": false,
    "latex-workshop.latex.clean.enabled": true,
    "latex-workshop.view.pdf.viewer": "external",
    "window.titleBarStyle": "custom",
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Default High Contrast",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile",
    // The command to execute when using external viewer.
    // This function is not officially supported. %PDF% is the placeholder for the absolute path to the generated PDF file.
    "latex-workshop.view.pdf.external.command": {
        "command": "qpdfview",
        "args": [
            "%PDF%"
        ]
    },
    // Files to clean.
    // This property must be an array of strings. File globs such as *.removeme, something?.aux can be used.
    "latex-workshop.latex.clean.fileTypes": [
        "*.acn",
        "*.acr",
        "*.alg",
        "*.aux",
        "*.bak*",
        "*.bbl",
        "*.bcf",
        "*.blg",
        "*.dvi",
        "*.fdb_latexmk",
        "*.fls",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.glsdefs",
        "*.hd",
        "*.idx",
        "*.ilg",
        "*.ind",
        "*.ins",
        "*.ist",
        "*.log",
        "*.loa",
        "*.loe",
        "*.lof",
        "*.lot",
        "*.mw",
        "*.nav",
        "*.out",
        "*.ps",
        "*.run.xml",
        "*.sbl",
        "*.slg",
        "*.slo",
        "*.sls",
        "*.snm",
        "*.sym",
        "*.synctex.gz",
        "*.toc",
        "*.vrb",
        "*.xdy"
    ],
    "latex-workshop.latexindent.path": "/usr/local/texlive/2018/bin/x86_64-linux/latexindent",
    "editor.fontFamily": "'IBM Plex Mono', 'Source Code Pro', 'Roboto Mono', 'monospace', monospace",
    "editor.fontSize": 13,
    "python.formatting.provider": "yapf",
    "terminal.external.linuxExec": "kitty"
}