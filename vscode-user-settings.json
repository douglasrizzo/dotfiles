{
    "editor.detectIndentation": true,
    "editor.fontFamily": "'IBM Plex Mono', 'Source Code Pro', 'Roboto Mono', 'monospace', monospace",
    "editor.fontSize": 16,
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.latexindent.path": "/home/dodo/.texlive/2019/bin/x86_64-linux/latexindent",
    "latex-workshop.view.pdf.viewer": "external",
    "python.formatting.provider": "yapf",
    "spellright.addToSystemDictionary": false,
    "terminal.external.linuxExec": "kitty",
    "workbench.activityBar.visible": false,
    // "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile",
    // The command to execute when using external viewer.
    // This function is not officially supported. %PDF% is the placeholder for the absolute path to the generated PDF file.
    "latex-workshop.view.pdf.external.command": {
        "command": "evince",
        "args": [
            "%PDF%"
        ]
    },
    // Files to clean.
    // This property must be an array of strings. File globs such as *.removeme, something?.aux can be used.
    "latex-workshop.latex.clean.fileTypes": [
        "*.acn",
        "*.acr",
        "*.alg",
        "*.aux",
        "*.bak*",
        "*.bbl",
        "*.bcf",
        "*.blg",
        "*.dvi",
        "*.fdb_latexmk",
        "*.fls",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.glsdefs",
        "*.glstex",
        "*.hd",
        "*.idx",
        "*.ilg",
        "*.ind",
        "*.ins",
        "*.ist",
        "*.log",
        "*.loa",
        "*.loe",
        "*.lof",
        "*.lot",
        "*.mw",
        "*.nav",
        "*.out",
        "*.ps",
        "*.run.xml",
        "*.sbl",
        "*.slg",
        "*.slo",
        "*.sls",
        "*.snm",
        "*.sym",
        "*.synctex.gz",
        "*.toc",
        "*.vrb",
        "*.xdy"
    ],
    "spellright.documentTypes": [
        "markdown",
        "plaintext",
        "latex"
    ],
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-pdf",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "makeglossaries",
            "command": "makeglossaries",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "bib2gls",
            "command": "bib2gls",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "texindy",
            "command": "texindy",
            "args": [
                "%DOCFILE%.idx"
            ],
            "env": {}
        }
    ],
    "workbench.colorTheme": "Monokai"
}