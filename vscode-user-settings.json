{
    "editor.detectIndentation": true,
    "editor.fontFamily": "'B612 Mono', 'monospace', 'Font Awesome 5 Free', 'Font Awesome 5 Free Solid', 'Font Awesome 5 Brands'",
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "on",
    "explorer.confirmDelete": false,
    "git.autofetch": true,
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.view.pdf.viewer": "external",
    "python.formatting.provider": "yapf",
    "spellright.addToSystemDictionary": false,
    "terminal.external.linuxExec": "kitty",
    "workbench.activityBar.visible": false,
    // "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile",
    // Files to clean.
    // This property must be an array of strings. File globs such as *.removeme, something?.aux can be used.
    "latex-workshop.latex.clean.fileTypes": [
        "*.acn",
        "*.acr",
        "*.alg",
        "*.aux",
        "*.bak*",
        "*.bbl",
        "*.bcf",
        "*.blg",
        "*.dvi",
        "*.fdb_latexmk",
        "*.fls",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.glsdefs",
        "*.glstex",
        "*.hd",
        "*.idx",
        "*.ilg",
        "*.ind",
        "*.ins",
        "*.ist",
        "*.log",
        "*.loa",
        "*.loe",
        "*.lof",
        "*.lot",
        "*.mw",
        "*.nav",
        "*.out",
        "*.ps",
        "*.run.xml",
        "*.sbl",
        "*.slg",
        "*.slo",
        "*.sls",
        "*.snm",
        "*.sym",
        "*.synctex.gz",
        "*.toc",
        "*.vrb",
        "*.xdy"
    ],
    "spellright.language": [
        "en_US",
        "pt_BR"
    ],
    "spellright.documentTypes": [
        "markdown",
        "plaintext",
        "latex"
    ],
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-pdf",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "makeglossaries",
            "command": "makeglossaries",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "bib2gls",
            "command": "bib2gls",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "texindy",
            "command": "texindy",
            "args": [
                "%DOCFILE%.idx"
            ],
            "env": {}
        }
    ],
    "autoDocstring.docstringFormat": "sphinx",
    "editor.suggestSelection": "first",
    "editor.fontSize": 15,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "python.pythonPath": "/home/dodo/.anaconda3/bin/python",
    "python.dataScience.askForKernelRestart": false,
    "python.analysis.cacheFolderPath": "~/.vscode-python-cache",
    "python.autoComplete.addBrackets": true,
    "python.venvPath": "~/.virtualenvs",
    "workbench.colorTheme": "Ayu Dark",
    "workbench.iconTheme": "ayu",
    "C_Cpp.clang_format_fallbackStyle": "Chromium",
    "editor.formatOnSave": true,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorSurroundingLines": 3,
    "files.insertFinalNewline": true,
}